# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'CCPod' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for CCPod
   pod 'Alamofire' #not specifying version number will install the latest version
   
   # Installing snap kit - Autolayout third party library
   pod 'SnapKit'
   
   # kingfisher - photo processing library
   pod 'Kingfisher'
   

end
